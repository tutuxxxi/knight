<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTDMapper3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cdu.linmu.knight.mapper.UserMapper">
	<select id="selectByTel" resultType="cdu.linmu.knight.entity.User">
		select *
		from tb_user
		where tel = #{tel}
	</select>
</mapper>